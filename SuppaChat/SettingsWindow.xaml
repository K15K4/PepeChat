<Window x:Class="SuppaChat.SettingsWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:SuppaChat"
        mc:Ignorable="d"
        Title="SettingsWindow" Height="600" Width="746">
    <Grid Background="#dff7f0">
        <Border  MinWidth="400" Margin="15"  Padding="30"  CornerRadius="20" Background="#adf7e1">
            <StackPanel>
                <TabControl Background="#6ffcd2" TabStripPlacement="Left">
                    <TabItem Name="zhopa" Header="zhopa" Background="#6ffcd2">
                        <Button x:Name="LoginButton" HorizontalAlignment="Left" Content="Login" Width="218" />
                        
                    </TabItem>
                    <TabItem Name="g" Header="fnjf"/>
                </TabControl>
            </StackPanel>
        </Border>
    </Grid>
</Window>
