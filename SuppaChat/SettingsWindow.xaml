<Window x:Class="SuppaChat.SettingsWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:SuppaChat"
        mc:Ignorable="d"
        Title="SettingsWindow" Height="600" Width="746">
    <Grid Grid.Row="2">
        <Grid.Background>
            <LinearGradientBrush StartPoint="0,0" EndPoint="0,1">
                <GradientStop Color="#252C32" Offset="0"></GradientStop>
                    
                <GradientStop Color="#1D1E20" Offset="2"></GradientStop>
                
            </LinearGradientBrush>
        </Grid.Background>
    </Grid>
</Window>
